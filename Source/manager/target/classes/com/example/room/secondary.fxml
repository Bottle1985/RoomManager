<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.room.Controller.SecondaryController">
    <children>
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Payment Management" />
        <Button fx:id="secondaryButton" onAction="#switchToPaymentPage" text="Payment Management" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Button fx:id="roomManagementDetailButton" onAction="#switchroomManagementDetail" text="Room Management" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    
    <children>
      <Button fx:id="btnCustomer" mnemonicParsing="false" onAction="#switchToCustomerDetail" text="Customers" />
        <Button fx:id="bookingButton" onAction="#switchToBookingPage" text="Booking Management" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
